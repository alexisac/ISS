<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hospitalpharmacy.InterfaceRegisterPharmacist">
   <children>
      <Label layoutX="62.0" layoutY="25.0" text="REGISTER PHARMACIST">
         <font>
            <Font name="Georgia Bold" size="36.0" />
         </font>
      </Label>
      <TextField fx:id="nameText" layoutX="184.0" layoutY="97.0" promptText="Doctor name">
         <font>
            <Font name="Georgia" size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="codeText" layoutX="184.0" layoutY="141.0" promptText="Doctor code">
         <font>
            <Font name="Georgia" size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="confirmedCodeText" layoutX="185.0" layoutY="184.0" promptText="Repeat doctor code">
         <font>
            <Font name="Georgia" size="18.0" />
         </font>
      </TextField>
      <Button layoutX="185.0" layoutY="255.0" mnemonicParsing="false" onAction="#signUp" text="Sign up">
         <font>
            <Font name="Georgia Bold" size="18.0" />
         </font>
      </Button>
      <Button layoutX="320.0" layoutY="255.0" mnemonicParsing="false" onAction="#logout" text="Go back">
         <font>
            <Font name="Georgia Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
